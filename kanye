                  , i = function() {
                    if (null !== e.getAttribute("data-color")) {
                        var t = e.getAttribute("data-color").split(" ");
                        n.style.setProperty("--body-color-change", t)
                    } else
                        n.style.setProperty("--body-color-change", "#370A41")
