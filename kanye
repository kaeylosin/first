            CatmullRomCurve3: class extends Ao {
                constructor(e=[], t=!1, n="centripetal", i=.5) {
                    super(),
                    this.isCatmullRomCurve3 = !0,
                    this.type = "CatmullRomCurve3",
                    this.points = e,
                    this.closed = t,
                    this.curveType = n,
                    this.tension = i
                }
                getPoint(e, t=new It) {
                    const n = t
                      , i = this.points
                      , r = i.length
                      , s = (r - (this.closed ? 0 : 1)) * e;
                    let a, o, l = Math.floor(s), c = s - l;
                    this.closed ? l += l > 0 ? 0 : (Math.floor(Math.abs(l) / r) + 1) * r : 0 === c && l === r - 1 && (l = r - 2,
                    c = 1),
                    this.closed || l > 0 ? a = i[(l - 1) % r] : (Co.subVectors(i[0], i[1]).add(i[0]),
                    a = Co);
                    const u = i[l % r]
                      , h = i[(l + 1) % r];
                    if (this.closed || l + 2 < r ? o = i[(l + 2) % r] : (Co.subVectors(i[r - 1], i[r - 2]).add(i[r - 1]),
                    o = Co),
                    "centripetal" === this.curveType || "chordal" === this.curveType) {
                        const e = "chordal" === this.curveType ? .5 : .25;
                        let t = Math.pow(a.distanceToSquared(u), e)
                          , n = Math.pow(u.distanceToSquared(h), e)
                          , i = Math.pow(h.distanceToSquared(o), e);
                        n < 1e-4 && (n = 1),
                        t < 1e-4 && (t = n),
                        i < 1e-4 && (i = n),
                        Po.initNonuniformCatmullRom(a.x, u.x, h.x, o.x, t, n, i),
                        Uo.initNonuniformCatmullRom(a.y, u.y, h.y, o.y, t, n, i),
                        Io.initNonuniformCatmullRom(a.z, u.z, h.z, o.z, t, n, i)
                    } else
                        "catmullrom" === this.curveType && (Po.initCatmullRom(a.x, u.x, h.x, o.x, this.tension),
                        Uo.initCatmullRom(a.y, u.y, h.y, o.y, this.tension),
                        Io.initCatmullRom(a.z, u.z, h.z, o.z, this.tension));
                    return n.set(Po.calc(c), Uo.calc(c), Io.calc(c)),
                    n
                }
                copy(e) {
                    super.copy(e),
                    this.points = [];
