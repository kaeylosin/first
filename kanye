                t.push(s.i / n | 0),
                pl(i),
                pl(i.next),
                i = e = s),
                i = i.next
            } while (i !== e);
            return Xo(i)
        }
        function Zo(e, t, n, i, r, s) {
            let a = e;
            do {
                let e = a.next.next;
                for (; e !== a.prev; ) {
                    if (a.i !== e.i && rl(a, e)) {
                        let o = hl(a, e);
                        return a = Xo(a, a.next),
                        o = Xo(o, o.next),
