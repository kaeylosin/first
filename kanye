        function Iu(e, t, n, i, r) {
            const s = r.glyphs[e] || r.glyphs["?"];
            if (!s)
                return void console.error('THREE.Font: character "' + e + '" does not exists in font family ' + r.familyName + ".");
            const a = new Mc;
            let o, l, c, u, h, d, p, f;
            if (s.o) {
                const e = s._cachedOutline || (s._cachedOutline = s.o.split(" "));
                for (let r = 0, s = e.length; r < s; )
                    switch (e[r++]) {
                    case "m":
                        o = e[r++] * t + n,
                        l = e[r++] * t + i,
                        a.moveTo(o, l);
                        break;
                    case "l":
                        o = e[r++] * t + n,
                        l = e[r++] * t + i,
                        a.lineTo(o, l);
                        break;
                    case "q":
                        c = e[r++] * t + n,
                        u = e[r++] * t + i,
                        h = e[r++] * t + n,
                        d = e[r++] * t + i,
                        a.quadraticCurveTo(h, d, c, u);
                        break;
                    case "b":
                        c = e[r++] * t + n,
                        u = e[r++] * t + i,
                        h = e[r++] * t + n,
                        d = e[r++] * t + i,
                        p = e[r++] * t + n,
                        f = e[r++] * t + i,
                        a.bezierCurveTo(h, d, p, f, c, u)
                    }
            }
            return {
                offsetX: s.ha * t,
                path: a
            }
        }
