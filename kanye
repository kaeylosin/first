        }
        function ll(e, t, n) {
            return t.x <= Math.max(e.x, n.x) && t.x >= Math.min(e.x, n.x) && t.y <= Math.max(e.y, n.y) && t.y >= Math.min(e.y, n.y)
        }
        function cl(e) {
            return e > 0 ? 1 : e < 0 ? -1 : 0
        }
        function ul(e, t) {
            return sl(e.prev, e, e.next) < 0 ? sl(e, t, e.next) >= 0 && sl(e, e.prev, t) >= 0 : sl(e, t, e.prev) < 0 || sl(e, e.next, t) < 0
        }
        function hl(e, t) {
