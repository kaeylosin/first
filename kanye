                getPoint(e, t=new It) {
                    const n = t;
                    return 1 === e ? n.copy(this.v2) : (n.copy(this.v2).sub(this.v1),
                    n.multiplyScalar(e).add(this.v1)),
                    n
                }
                getPointAt(e, t) {
                    return this.getPoint(e, t)
                }
                getTangent(e, t=new It) {
                    return t.subVectors(this.v2, this.v1).normalize()
                }
                getTangentAt(e, t) {
                    return this.getTangent(e, t)
                }
