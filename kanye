                    h = u)),
                    i = i.next
                } while (i !== o);
                return n
            }(e, t);
            if (!n)
                return t;
            const i = hl(n, e);
            return Xo(i, i.next),
            Xo(n, n.next)
        }
        function el(e, t) {
            return sl(e.prev, e, t.prev) < 0 && sl(t.next, e, e.next) < 0
        }
        function tl(e, t, n, i, r) {
            return (e = 1431655765 & ((e = 858993459 & ((e = 252645135 & ((e = 16711935 & ((e = (e - n) * r | 0) | e << 8)) | e << 4)) | e << 2)) | e << 1)) | (t = 1431655765 & ((t = 858993459 & ((t = 252645135 & ((t = 16711935 & ((t = (t - i) * r | 0) | t << 8)) | t << 4)) | t << 2)) | t << 1)) << 1
        }
        function nl(e) {
            let t = e
              , n = e;
            do {
                (t.x < n.x || t.x === n.x && t.y < n.y) && (n = t),
                t = t.next
            } while (t !== e);
            return n
        }
        function il(e, t, n, i, r, s, a, o) {
            return (r - a) * (t - o) >= (e - a) * (s - o) && (e - a) * (i - o) >= (n - a) * (t - o) && (n - a) * (s - o) >= (r - a) * (i - o)
        }
        function rl(e, t) {
            return e.next.i !== t.i && e.prev.i !== t.i && !function(e, t) {
