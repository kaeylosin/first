            a = qs(a, n),
            a = Ys(a, n),
            o = Js(o),
            o = qs(o, n),
            o = Ys(o, n),
            a = ea(a),
            o = ea(o),
            n.isWebGL2 && !0 !== n.isRawShaderMaterial && (_ = "#version 300 es\n",
