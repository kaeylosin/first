        class Ns {
            constructor(e, t, n) {
                this.id = e,
                this.addr = n,
                this.cache = [],
                this.setValue = function(e) {
                    switch (e) {
                    case 5126:
                        return Kr;
                    case 35664:
                        return Jr;
                    case 35665:
                        return Zr;
                    case 35666:
                        return Qr;
