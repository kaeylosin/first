                    r = new WeakMap
                }
            }
        }
        const Or = new wt
          , Fr = new Ct
          , Br = new Pt
          , zr = new Xi
          , kr = []
          , Hr = []
          , Vr = new Float32Array(16)
          , Gr = new Float32Array(9)
          , Wr = new Float32Array(4);
        function jr(e, t, n) {
            const i = e[0];
            if (i <= 0 || i > 0)
                return e;
            const r = t * n;
            let s = kr[r];
            if (void 0 === s && (s = new Float32Array(r),
            kr[r] = s),
            0 !== t) {
                i.toArray(s, 0);
                for (let i = 1, r = 0; i !== t; ++i)
                    r += n,
                    e[i].toArray(s, r)
            }
            return s
        }
        function Xr(e, t) {
            if (e.length !== t.length)
                return !1;
            for (let n = 0, i = e.length; n < i; n++)
                if (e[n] !== t[n])
                    return !1;
            return !0
        }
        function qr(e, t) {
            for (let n = 0, i = t.length; n < i; n++)
                e[n] = t[n]
        }
        function Yr(e, t) {
            let n = Hr[t];
            void 0 === n && (n = new Int32Array(t),
            Hr[t] = n);
