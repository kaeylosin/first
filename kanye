            }
            copySampleValue_(e) {
                const t = this.resultBuffer
                  , n = this.sampleValues
                  , i = this.valueSize
                  , r = e * i * 3 + i;
                for (let e = 0; e !== i; e++)
                    t[e] = n[r + e];
                return t
            }
            interpolate_(e, t, n, i) {
                const r = this.resultBuffer
                  , s = this.sampleValues
                  , a = this.valueSize
                  , o = 2 * a
                  , l = 3 * a
                  , c = i - t
                  , u = (n - t) / c
                  , h = u * u
                  , d = h * u
                  , p = e * l
                  , f = p - l
                  , m = -2 * d + 3 * h
                  , g = d - h
                  , v = 1 - m
                  , _ = g - h + u;
                for (let e = 0; e !== a; e++) {
                    const t = s[f + e + a]
                      , n = s[f + e + o] * c
                      , i = s[p + e + a]
                      , l = s[p + e] * c;
                    r[e] = v * t + _ * n + m * i + g * l
                }
                return r
            }
        }
