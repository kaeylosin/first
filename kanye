                            if (!(l === re || Te.convert(l) === Re.getParameter(Re.IMPLEMENTATION_COLOR_READ_TYPE) || l === oe && (K.isWebGL2 || Y.has("OES_texture_float") || Y.has("WEBGL_color_buffer_float")) || c))
