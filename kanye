        class fr extends Vi {
            constructor(e=-1, t=1, n=1, i=-1, r=.1, s=2e3) {
                super(),
                this.isOrthographicCamera = !0,
                this.type = "OrthographicCamera",
                this.zoom = 1,
                this.view = null,
                this.left = e,
