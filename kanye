                }
                return null !== a && (a.visible = null !== i),
                null !== o && (o.visible = null !== r),
                null !== l && (l.visible = null !== s),
                this
            }
            _getHandJoint(e, t) {
                if (void 0 === e.joints[t.jointName]) {
                    const n = new wa;
                    n.matrixAutoUpdate = !1,
                    n.visible = !1,
                    e.joints[t.jointName] = n,
                    e.add(n)
                }
                return e.joints[t.jointName]
            }
        }
        class La extends wt {
