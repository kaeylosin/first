        }
        function sl(e, t, n) {
            return (t.y - e.y) * (n.x - t.x) - (t.x - e.x) * (n.y - t.y)
        }
        function al(e, t) {
            return e.x === t.x && e.y === t.y
        }
        function ol(e, t, n, i) {
            const r = cl(sl(e, t, n))
              , s = cl(sl(e, t, i))
