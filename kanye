                this.onStart = void 0,
                this.onLoad = e,
                this.onProgress = t,
                this.onError = n,
                this.itemStart = function(e) {
                    o++,
                    !1 === s && void 0 !== i.onStart && i.onStart(e, a, o),
                    s = !0
                }
                ,
                this.itemEnd = function(e) {
                    a++,
                    void 0 !== i.onProgress && i.onProgress(e, a, o),
                    a === o && (s = !1,
                    void 0 !== i.onLoad && i.onLoad())
                }
                ,
