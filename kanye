        const pt = (new at).set(.8224621, .177538, 0, .0331941, .9668058, 0, .0170827, .0723974, .9105199)
          , ft = (new at).set(1.2249401, -.2249404, 0, -.0420569, 1.0420571, 0, -.0196376, -.0786361, 1.0982735)
          , mt = {
            [Ee]: {
                transfer: Ae,
                primaries: Le,
                toReference: e => e,
                fromReference: e => e
            },
            [be]: {
                transfer: Re,
                primaries: Le,
                toReference: e => e.convertSRGBToLinear(),
                fromReference: e => e.convertLinearToSRGB()
            },
            [we]: {
                transfer: Ae,
                primaries: Ce,
