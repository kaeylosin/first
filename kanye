            }
            static CreateFromMorphTargetSequence(e, t, n, i) {
                const r = t.length
                  , s = [];
                for (let e = 0; e < r; e++) {
                    let a = []
                      , o = [];
                    a.push((e + r - 1) % r, e, (e + 1) % r),
                    o.push(0, 1, 0);
                    const l = Sl(a);
                    a = bl(a, 1, l),
                    o = bl(o, 1, l),
                    i || 0 !== a[0] || (a.push(r),
