                    t.to(r, {
                        opacity: 1,
                        ease: "Power1.easeNone"
                    }, "<25%"),
                    ScrollTrigger.create({
                        trigger: e,
                        start: "top 0%",
                        toggleActions: "restart none none reset",
                        end: "+=200%",
                        pin: !0,
                        scrub: .5,
