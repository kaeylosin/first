                    return void 0 === s ? (a = new va(e,t),
                    n.set(i, [a])) : r >= s.length ? (a = new va(e,t),
                    s.push(a)) : a = s[r],
                    a
                },
                dispose: function() {
                    n = new WeakMap
                }
            }
        }
        class ya extends Kn {
            constructor(e) {
                super(),
                this.isMeshDepthMaterial = !0,
