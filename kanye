        class Il extends Tl {
            constructor(e, t, n, i) {
                super(e, t, n, i)
            }
            interpolate_(e, t, n, i) {
                const r = this.resultBuffer
                  , s = this.sampleValues
                  , a = this.valueSize
                  , o = (n - t) / (i - t);
                let l = e * a;
                for (let e = l + a; l !== e; l += 4)
                    Ut.slerpFlat(r, 0, s, l - a, s, l, o);
