                function o() {
                    c(),
                    zl.add(e, this),
                    t && t(this),
                    r.manager.itemEnd(e)
                }
                function l(t) {
                    c(),
                    i && i(t),
                    r.manager.itemError(e),
                    r.manager.itemEnd(e)
                }
                function c() {
                    a.removeEventListener("load", o, !1),
                    a.removeEventListener("error", l, !1)
                }
                return a.addEventListener("load", o, !1),
                a.addEventListener("error", l, !1),
