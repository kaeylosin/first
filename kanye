        class qn {
            constructor(e=new It, t=new It, n=new It) {
                this.a = e,
                this.b = t,
                this.c = n
            }
            static getNormal(e, t, n, i) {
                i.subVectors(n, t),
                On.subVectors(e, t),
                i.cross(On);
                const r = i.lengthSq();
                return r > 0 ? i.multiplyScalar(1 / Math.sqrt(r)) : i.set(0, 0, 0)
            }
            static getBarycoord(e, t, n, i, r) {
                On.subVectors(i, t),
                Fn.subVectors(n, t),
                Bn.subVectors(e, t);
                const s = On.dot(On)
                  , a = On.dot(Fn)
                  , o = On.dot(Bn)
                  , l = Fn.dot(Fn)
                  , c = Fn.dot(Bn)
                  , u = s * l - a * a;
                if (0 === u)
                    return r.set(-2, -1, -1);
                const h = 1 / u
                  , d = (l * o - a * c) * h
                  , p = (s * c - a * o) * h;
                return r.set(1 - d - p, p, d)
            }
            static containsPoint(e, t, n, i) {
                return this.getBarycoord(e, t, n, i, zn),
                zn.x >= 0 && zn.y >= 0 && zn.x + zn.y <= 1
            }
            static getUV(e, t, n, i, r, s, a, o) {
                return !1 === Xn && (console.warn("THREE.Triangle.getUV() has been renamed to THREE.Triangle.getInterpolation()."),
                Xn = !0),
                this.getInterpolation(e, t, n, i, r, s, a, o)
            }
            static getInterpolation(e, t, n, i, r, s, a, o) {
                return this.getBarycoord(e, t, n, i, zn),
                o.setScalar(0),
                o.addScaledVector(r, zn.x),
                o.addScaledVector(s, zn.y),
                o.addScaledVector(a, zn.z),
                o
            }
            static isFrontFacing(e, t, n, i) {
                return On.subVectors(n, t),
                Fn.subVectors(e, t),
                On.cross(Fn).dot(i) < 0
            }
            set(e, t, n) {
                return this.a.copy(e),
                this.b.copy(t),
                this.c.copy(n),
                this
            }
            setFromPointsAndIndices(e, t, n, i) {
                return this.a.copy(e[t]),
                this.b.copy(e[n]),
                this.c.copy(e[i]),
                this
            }
            setFromAttributeAndIndices(e, t, n, i) {
                return this.a.fromBufferAttribute(e, t),
                this.b.fromBufferAttribute(e, n),
                this.c.fromBufferAttribute(e, i),
                this
            }
            clone() {
                return (new this.constructor).copy(this)
            }
            copy(e) {
                return this.a.copy(e.a),
                this.b.copy(e.b),
                this.c.copy(e.c),
                this
            }
            getArea() {
                return On.subVectors(this.c, this.b),
                Fn.subVectors(this.a, this.b),
                .5 * On.cross(Fn).length()
            }
            getMidpoint(e) {
                return e.addVectors(this.a, this.b).add(this.c).multiplyScalar(1 / 3)
            }
            getNormal(e) {
                return qn.getNormal(this.a, this.b, this.c, e)
            }
            getPlane(e) {
                return e.setFromCoplanarPoints(this.a, this.b, this.c)
            }
            getBarycoord(e, t) {
                return qn.getBarycoord(e, this.a, this.b, this.c, t)
            }
            getUV(e, t, n, i, r) {
                return !1 === Xn && (console.warn("THREE.Triangle.getUV() has been renamed to THREE.Triangle.getInterpolation()."),
                Xn = !0),
                qn.getInterpolation(e, this.a, this.b, this.c, t, n, i, r)
            }
            getInterpolation(e, t, n, i, r) {
                return qn.getInterpolation(e, this.a, this.b, this.c, t, n, i, r)
            }
            containsPoint(e) {
                return qn.containsPoint(e, this.a, this.b, this.c)
            }
            isFrontFacing(e) {
                return qn.isFrontFacing(this.a, this.b, this.c, e)
            }
            intersectsBox(e) {
                return e.intersectsTriangle(this)
            }
            closestPointToPoint(e, t) {
                const n = this.a
                  , i = this.b
                  , r = this.c;
                let s, a;
                kn.subVectors(i, n),
                Hn.subVectors(r, n),
                Gn.subVectors(e, n);
                const o = kn.dot(Gn)
                  , l = Hn.dot(Gn);
                if (o <= 0 && l <= 0)
                    return t.copy(n);
                Wn.subVectors(e, i);
                const c = kn.dot(Wn)
                  , u = Hn.dot(Wn);
                if (c >= 0 && u <= c)
                    return t.copy(i);
                const h = o * u - c * l;
                if (h <= 0 && o >= 0 && c <= 0)
                    return s = o / (o - c),
                    t.copy(n).addScaledVector(kn, s);
                jn.subVectors(e, r);
                const d = kn.dot(jn)
                  , p = Hn.dot(jn);
                if (p >= 0 && d <= p)
                    return t.copy(r);
                const f = d * l - o * p;
                if (f <= 0 && l >= 0 && p <= 0)
                    return a = l / (l - p),
                    t.copy(n).addScaledVector(Hn, a);
                const m = c * p - d * u;
                if (m <= 0 && u - c >= 0 && d - p >= 0)
                    return Vn.subVectors(r, i),
                    a = (u - c) / (u - c + (d - p)),
                    t.copy(i).addScaledVector(Vn, a);
                const g = 1 / (m + f + h);
                return s = f * g,
                a = h * g,
                t.copy(n).addScaledVector(kn, s).addScaledVector(Hn, a)
            }
            equals(e) {
                return e.a.equals(this.a) && e.b.equals(this.b) && e.c.equals(this.c)
            }
        }
        let Yn = 0;
        class Kn extends je {
            constructor() {
                super(),
                this.isMaterial = !0,
                Object.defineProperty(this, "id", {
                    value: Yn++
                }),
                this.uuid = Ke(),
                this.name = "",
                this.type = "Material",
                this.blending = 1,
                this.side = 0,
                this.vertexColors = !1,
                this.opacity = 1,
                this.transparent = !1,
                this.alphaHash = !1,
