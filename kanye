                        void qo(o, t, n, i, r, s, 0)
                    }
                    e = e.next
                }
                a = a.next
            } while (a !== e)
        }
        function Qo(e, t) {
            return e.x - t.x
        }
        function $o(e, t) {
            const n = function(e, t) {
                let n, i = t, r = -1 / 0;
                const s = e.x
                  , a = e.y;
                do {
